{
  "name": "ucdapi.org",
  "version": "1.0.0",
  "description": "ucdapi.org frontend",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack/webpack.config.dev.js",
    "build": "rm -rf dist && webpack --config webpack/webpack.config.prod.js  && cp -a assets/ dist/",
    "prod": "serve ./dist -s",
    "lint": "eslint src",
    "ngrok": "ngrok http -region eu 8080",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --single-quote --trailing-comma all",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mithrandi/ucdapi.git"
  },
  "keywords": [
    "react",
    "redux",
    "async"
  ],
  "author": "Tristan Seligmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mithrandi/ucdapi/issues"
  },
  "homepage": "https://github.com/mithrandi/ucdapi",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-emotion": "^8.0.9",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "preload-webpack-plugin": "^2.0.0",
    "prettier": "^1.7.4",
    "react-test-renderer": "^16.0.0",
    "resolve-url-loader": "^2.2.0",
    "serve": "^6.3.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-split-by-path": "^2.0.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "emotion": "^8.0.9",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-code-splitting": "^1.2.1",
    "react-dom": "^16.0.0",
    "react-emotion": "^8.0.9",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.1",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0"
  }
}
